<h1>Mis posts</h1>
<table>
  <tr> 
  	<th>Titulo</th>  
  	<th>Texto</th>  
  	<th></th>
  	<th></th>
    <th></th>
  </tr>

  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.text %></td>
      <td><%= link_to 'Mostrar', post_path(post) %></td>
  	  <td><%= link_to 'Editar', edit_post_path(post) %></td>
      <td><%= link_to 'Eliminar', post_path(post),
  method: :delete, data: { confirm: '¿Estás seguro?' } %></td>

    </tr>
  <% end %>
</table>

<%= link_to 'Nuevo post', new_post_path %>
